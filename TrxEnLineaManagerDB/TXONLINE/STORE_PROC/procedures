CREATE OR REPLACE FUNCTION get_bal (acc_no IN NUMBER)
  RETURN NUMBER
   IS
       acc_bal NUMBER(11,2);    
BEGIN
     SELECT count(*) INTO acc_bal FROM trx_linea.tpos_header;
          
       RETURN(acc_bal);
END;
/

