CREATE OR REPLACE FUNCTION ComparaVentasMediosdePago (fecha IN DATE, almacen IN NUMBER, terminal IN NUMBER, transaccion IN NUMBER, mensaje OUT VARCHAR2)
  RETURN NUMBER
   IS
       acc_bal NUMBER(11,2);    
BEGIN
		SELECT count(*) INTO acc_bal FROM trx_linea.tpos_header;
     	mensaje:=TO_CHAR(acc_bal);
          
       RETURN(0);
END;
/
